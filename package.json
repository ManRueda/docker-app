{
  "name": "basic-electron-react-boilerplate",
  "version": "0.4.0",
  "description": "Minimal and modern react+electron+webpack boilerplate",
  "author": "Phillip Barbiero",
  "homepage": "https://github.com/ManRueda/basic-electron-react-boilerplate",
  "repository": "https://github.com/ManRueda/basic-electron-react-boilerplate.git",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "testDev": "electron .",
    "testProd": "electron . --noDevServer",
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
    "build": "webpack --config webpack.build.config.js",
    "package": "webpack --config webpack.build.config.js",
    "postpackage": "electron-packager ./ --out=./builds",
    "lint": "standard"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babili-webpack-plugin": "^0.0.11",
    "css-loader": "^0.26.2",
    "electron": "^1.6.1",
    "electron-devtools-installer": "^2.2.0",
    "electron-packager": "^8.5.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "redux-devtools": "^3.4.0",
    "shebang-loader": "^0.0.1",
    "standard": "^10.0.2",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "dockerode": "^2.4.3",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "redux": "^3.6.0"
  }
}
